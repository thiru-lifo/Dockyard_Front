import { OnInit, ElementRef, AfterViewInit, OnDestroy } from '@angular/core';
import MetisMenu from 'metismenujs';
import { IMMOptions } from 'metismenujs/dist/types/interface';
export declare type toggleType = 'true' | 'false';
export declare class MetismenuAngularComponent implements OnInit, AfterViewInit, OnDestroy {
    private elem;
    toggle: toggleType;
    parentTrigger: string;
    subMenu: string;
    triggerElement: string;
    protected mm: MetisMenu;
    mmOpts: IMMOptions;
    constructor(elem: ElementRef);
    ngAfterViewInit(): void;
    ngOnInit(): void;
    ngOnDestroy(): void;
}
